{
  "name": "svg-to-3d-mesh",
  "version": "0.0.1",
  "author": {
    "name": "Anh Nguyen",
    "email": "anhnicky66@gmail.com",
    "url": "https://github.com/anhnicky66"
  },
  "description": "Render SVG path into 3d triangulated mesh",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/svg-to-3d-mesh/svg-to-3d-mesh.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "canvas-loop": "^1.0.7",
    "extract-svg-path": "^2.1.0",
    "gl-vec3": "^1.1.3",
    "mesh-reindex": "^1.0.0",
    "svg-mesh-3d": "^1.1.0",
    "three": "^0.118.3",
    "three-simplicial-complex": "^69.0.6",
    "triangle-centroid": "^1.0.0",
    "tweenr": "^2.2.1",
    "unindex-mesh": "^2.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "npm-force-resolutions": "0.0.3",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "keywords": [
    "svg",
    "3d",
    "mesh",
    "web-components"
  ],
  "homepage": "https://github.com/anhnicky66/svg-to-3d-mesh"
